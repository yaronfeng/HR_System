
@{
    ViewBag.Title = "新增员工";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript" src="/JS/Employee/EmployeeAdd.js"></script>


<div class="am-cf am-padding am-padding-bottom-0">
    <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">新增员工</strong> / <small>Personal information</small></div>
</div>
<hr />
<div class="am-g">
    <div class="am-u-sm-12 am-u-sm-centered">
        <div class="am-panel am-panel-default" id="jqxValidator">
            <!---001 start-->
            <div class="p1">
                <ul>
                    <li>
                        <span>姓名：</span>
                        <div class="txb">
                            <input type="text" id="txbEmpName" />
                        </div>
                    </li>
                    <li>
                        <span>所属企业：</span>
                        <div class="txb" id="selCorpId">
                        </div>
                    </li>
                    <li>
                        <span>供应商：</span>
                        <div class="txb" id="selSupId">
                        </div>
                    </li>
                    <li>
                        <span>性别：</span>
                        <div class="txb" id="selSex">
                        </div>
                    </li>
                    <li>
                        <span>身份证：</span>
                        <div class="txb">
                            <input type="text" id="txbCardNo" />
                        </div>
                    </li>
                    <li>
                        <span>地址：</span>
                        <div class="txb">
                            <input type="text" id="txbAddress" />
                        </div>
                    </li>
                    <li>
                        <span>联系电话：</span>
                        <div class="txb">
                            <input type="text" id="txbPhone" />
                        </div>
                    </li>
                   
                    <li>
                        <span>学历：</span>
                        <div class="txb" id="selDegree">
                        </div>
                    </li>
                    <li>
                        <span>岗位：</span>
                        <div class="txb">
                            <input type="text" id="txbJobs"/>
